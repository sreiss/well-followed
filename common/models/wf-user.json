{
  "name": "WfUser",
  "plural": "Users",
  "base": "User",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "institutionId": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "institution": {
      "type": "belongsTo",
      "model": "Institution",
      "foreignKey": "institutionId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
