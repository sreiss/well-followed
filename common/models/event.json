{
  "name": "Event",
  "plural": "Events",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "start": {
      "type": "date",
      "required": true
    },
    "end": {
      "type": "date",
      "required": true
    },
    "userId": {
      "type": "string",
      "required": true
    },
    "institutionId": {
      "type": "string",
      "required": true
    },
    "cancelled": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "WfUser",
      "foreignKey": "userId"
    },
    "institution": {
      "type": "belongsTo",
      "model": "Institution",
      "foreignKey": "institutionId"
    }
  },
  "acls": [],
  "methods": {}
}
